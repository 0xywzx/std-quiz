pragma solidity ^0.5.0;
pragma experimental ABIEncoderV2;

contract STD_Zombiee {
    
    struct Zombiee { //プロパティは18個
        uint256 id;
        string firstName;
        string middleName;
        string lastName;
        string favoriteFood;
        string placeOfBirth;
        string job;
        string nickName;
        uint256 age;
        uint256 height;
        uint256 weight;
        uint256 BMI;
        uint256 bloodPressure;
        uint256 footSize;
        uint256 motherAge;
        uint256 fatherAge;
        uint256 favoriteNumber;
        bool married;
    }

    uint256 internal id = 0;
    mapping (uint256 => Zombiee) private zombiees;
 
    function zombieeFactory( //引数は17個
        bytes memory _data
        ) public {
        id = id + 1;
        (
            string[] memory _stringData, 
            uint256[] memory _uintData,
            bool _married
        ) = abi.decode(_data, (
            string[], uint256[], bool
        ));
        Zombiee memory zombiee = Zombiee({ 
            id: id, 
            firstName: _stringData[0], 
            middleName: _stringData[1], 
            lastName: _stringData[2], 
            favoriteFood: _stringData[3], 
            placeOfBirth: _stringData[4], 
            job: _stringData[5], 
            nickName: _stringData[6], 
            age: _uintData[0], 
            height: _uintData[1], 
            weight: _uintData[2], 
            BMI: _uintData[3], 
            bloodPressure: _uintData[4], 
            footSize: _uintData[5], 
            motherAge: _uintData[6], 
            fatherAge: _uintData[7], 
            favoriteNumber: _uintData[8], 
            married: _married 
        });
        zombiees[id] = zombiee;
    }
    
    function getZombiee(uint256 _id) public view returns (Zombiee memory) {
        return zombiees[_id];
    }
    
}

